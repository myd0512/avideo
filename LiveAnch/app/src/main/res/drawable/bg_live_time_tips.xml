<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval"
            android:dither="true">
            <stroke android:width="1dp"
                android:color="@color/color_live_time" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="oval">
            <solid android:color="@color/color_live_time" />
            <size
                android:width="2dp"
                android:height="2dp" />
        </shape>
    </item>
</layer-list>