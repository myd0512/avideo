<?xml version="1.0" encoding="utf-8"?>
<com.yunbao.phonelive.custom.MyLinearLayout2 xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/margin_padding_64"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/img"
        android:layout_width="@dimen/margin_padding_30"
        android:layout_height="@dimen/margin_padding_30"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/margin_padding_8"
        android:padding="@dimen/margin_padding_2" />

    <TextView
        android:id="@+id/name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="@color/textColor"
        android:textSize="@dimen/text_12" />
</com.yunbao.phonelive.custom.MyLinearLayout2>